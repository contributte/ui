{
  "name": "contributte/ui",
  "description": "Contributte UI. Opinionated prime components for your Nette applications.",
  "keywords": [
    "nette",
    "contributte",
    "ui",
    "components",
    "controls",
    "frontend",
    "javascript"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/ui",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "latte/latte": "^3.0.12",
    "nette/di": "^3.1.8",
    "nette/utils": "^4.0.3"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.5",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.4",
    "contributte/phpstan": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\UI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "composer/package-versions-deprecated": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
